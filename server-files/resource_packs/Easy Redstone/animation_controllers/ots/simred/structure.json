{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.ots_simred.structure": {
      "states": {
        "default": {
          "transitions": [
            {
              "on": "q.property('ots_simred:structure.ui') == true"
            }
          ]
        },
        "on": {
          "animations": ["switch.on"],
          "transitions": [
            {
              "off": "q.property('ots_simred:structure.ui') == false"
            }
          ]
        },
        "off": {
          "animations": ["switch.off"],
          "transitions": [
            {
              "on": "q.property('ots_simred:structure.ui') == true"
            }
          ]
        }
      }
    }
  }
}
